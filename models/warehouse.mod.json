      "name": "location",
      "type": "string",
      "index": true,
      "nullable": true,
      "comment": "仓库位置"
    },
    {
      "label": "项目部",
      "name": "dept",
      "type": "string",
      "index": true,
      "nullable": true,
      "comment": "项目部"
    },
    {
      "label": "IP地址",
      "name": "ip",
      "type": "string",
      "nullable": true,
      "index": true,
      "comment": "IP地址"
    },
    {
