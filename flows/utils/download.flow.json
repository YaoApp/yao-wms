{
  "label": "下载RFID",
  "version": "1.0.0",
  "description": "下载RFID",
  "nodes": [
    {
      "name": "打印",
      "process": "xiang.helper.Print",
      "args": ["{{$in.0}}"]
    },
    {
      "name": "文件",
      "process": "scripts.utils.download",
      "args": ["{{$in.0}}", {}, 20]
    },
    {
      "name": "数据",
      "process": "xiang.fs.ReadFile",
      "args": ["{{$res.文件}}", false]
    },
    {
      "name": "打印",
      "process": "xiang.helper.Print",
      "args": ["{{$res.文件}}"]
    }
  ],
  "output": "{{$res.数据.content}}"
}
