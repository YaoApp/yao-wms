{
  "name": "单品接口",
  "version": "1.0.0",
  "description": "单品接口",
  "group": "product",
  "guard": "-",
  "paths": [
    {
      "path": "/find",
      "method": "GET",
      "process": "scripts.sku.GetOne",
      "in": ["$query.code", ":headers"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/save",
      "method": "POST",
      "process": "scripts.product.Save",
      "in": [":payload"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    }
  ]
}
