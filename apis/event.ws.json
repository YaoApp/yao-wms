{
  "name": "事件通知服务",
  "description": "事件通知服务",
  "version": "0.10.1",
  "protocols": ["yao-event-01"],
  "guard": "bearer-jwt",
  "buffer": { "read": 4096, "write": 4096 },
  "limit": { "read-wait": 300, "pong-wait": 300, "max-message": 104857 },
  "timeout": 5,
  "process": "scripts.event.Message"
}
